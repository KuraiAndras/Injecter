<uwpExample:MainPageBase
    x:Class="Injecter.UwpExample.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:uwpExample="using:Injecter.UwpExample"
    xmlns:viewModels="using:Injecter.UwpExample.ViewModels"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}"
    d:DataContext="{d:DesignInstance viewModels:IItemsViewModel, IsDesignTimeCreatable=False}">
    <Grid>
        <ListView
            ItemsSource="{Binding Items}" />
    </Grid>
</uwpExample:MainPageBase>
